;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	cc.pt g66pl06.cc.pt (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
;	name	servers	- ns records
@	IN	NS	ns.cc.pt.
@	IN	NS	ns2.cc.pt.

; PTR Records
2.2.10	IN	PTR	ns.cc.pt.  ;	10.2.2.1
3.3.10	IN	PTR	ns2.cc.pt  ; 	10.3.3.2
